<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Event Name</th>
      <th>Date</th>
      <th>Time</th>
      <th>Location</th>
      <th>Details</th>
    </tr>
  </thead>
  <tbody>
    <% events.forEach(function(m) { %>
      <tr>
        <td><%= m.eventName %></td>
        <td><%= m.date %></td>
        <td><%= m.time %></td>
        <td><%= m.location %></td>
        <td><%= m.details %></td>
        <td><a href="/events/<%= m._id %>">VIEW</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
